<Window x:Class="QLike.Foto.GridStudio.AboutWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="About FotoGrid Studio" Height="200" Width="380" WindowStartupLocation="CenterOwner" Icon="/FotoGridStudio;component/app_grid.ico">
    <Grid>
        <Button Height="23" Margin="0,0,11,12" Name="btnOK" VerticalAlignment="Bottom" Click="btnOK_Click" HorizontalAlignment="Right" Width="100">OK</Button>
        <Image Margin="12,12,0,0" Name="imageLogo" Stretch="Fill" HorizontalAlignment="Left" Width="48" Source="/FotoGridStudio;component/app_grid.ico" Height="48" VerticalAlignment="Top" MouseDown="imageLogo_MouseDown" />
        <Label Height="28" Margin="66,12,12,0" Name="lblAppName" VerticalAlignment="Top">FotoGrid Studio</Label>
        <Label Height="28" Margin="66,32,12,0" Name="lblPublisher" VerticalAlignment="Top">QLike.com</Label>
    </Grid>
</Window>
