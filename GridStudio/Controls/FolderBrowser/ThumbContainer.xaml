<UserControl x:Class="QLike.Foto.GridStudio.Controls.ThumbContainer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    HorizontalAlignment="Stretch" VerticalAlignment="Stretch" BorderBrush="Gray" BorderThickness="1">
    <ScrollViewer x:Name="scrolls" VerticalScrollBarVisibility="Visible" HorizontalScrollBarVisibility="Auto">
        <ScrollViewer.Content>
            <StackPanel Margin="0,5,0,0" Name="stackPanelMain">
            </StackPanel>
        </ScrollViewer.Content>
    </ScrollViewer>
</UserControl>
