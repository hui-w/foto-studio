<?xml version="1.0" encoding="utf-8" ?>
<FotoGrid CellSpacing="5">
  <Rows>
    <Row Height="*"></Row>
    <Row Height="*"></Row>
  </Rows>
  <Columns>
    <Column Width="*"></Column>
    <Column Width="10%"></Column>
    <Column Width="*"></Column>
  </Columns>
  <Cells>
    <Cell Row="0" Column="0" RowSpan="1" ColumnSpan="2"></Cell>
    <Cell Row="0" Column="2" RowSpan="1" ColumnSpan="1"></Cell>
    <Cell Row="1" Column="0" RowSpan="1" ColumnSpan="1"></Cell>
    <Cell Row="1" Column="1" RowSpan="1" ColumnSpan="2"></Cell>
  </Cells>
</FotoGrid>