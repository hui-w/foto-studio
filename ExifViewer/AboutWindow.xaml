<Window x:Class="QLike.Foto.ExifViewer.AboutWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="About Exif Viewer" Height="200" Width="380" WindowStartupLocation="CenterOwner" Icon="/ExifViewer;component/app_exif.ico">
    <Grid>
        <Button Height="23" Margin="0,0,11,12" Name="btnOK" VerticalAlignment="Bottom" Click="btnOK_Click" HorizontalAlignment="Right" Width="100">OK</Button>
        <Image Margin="12,12,0,0" Name="imageLogo" Stretch="Fill" HorizontalAlignment="Left" Width="48" Source="/ExifViewer;component/app_exif.ico" Height="48" VerticalAlignment="Top" />
        <Label Height="28" Margin="66,12,12,0" Name="lblAppName" VerticalAlignment="Top">Exif Viewer</Label>
        <Label Height="28" Margin="66,32,12,0" Name="lblPublisher" VerticalAlignment="Top">QLike.com</Label>
    </Grid>
</Window>
